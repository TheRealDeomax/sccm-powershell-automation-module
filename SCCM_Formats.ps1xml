<!-- Begin SMS_R_System Views -->

    <View>
        <Name>System.Management.ManagementObject#SMS_R_System</Name>
        <ViewSelectedBy>
            <TypeName>System.Management.ManagementObject#SMS_R_System</TypeName>
        </ViewSelectedBy>
        <ListControl>
            <ListEntries>
                <ListEntry>
                    <ListItems>
                        <ListItem>
                            <PropertyName>Name</PropertyName>
                        </ListItem>
                        <ListItem>
                            <PropertyName>SMSAssignedSites</PropertyName>
                        </ListItem>
                        <ListItem>
                            <PropertyName>ClientVersion</PropertyName>
                        </ListItem>
                        <ListItem>
                            <Label>CreationDate</Label>
                            <ScriptBlock>$_.CreationDate | Convert-SCCMDateToDate</ScriptBlock>
                        </ListItem>
                        <ListItem>
                            <PropertyName>ResourceID</PropertyName>
                        </ListItem>
                        <ListItem>

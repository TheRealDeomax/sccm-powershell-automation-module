            <TableHeaders>
                <TableColumnHeader>
                    <Label>Name</Label>
                    <Width>25</Width>
                </TableColumnHeader>
                <TableColumnHeader>
                    <Label>ResourceID</Label>
                    <Alignment>Right</Alignment>
                    <Width>20</Width>
                </TableColumnHeader>
                <TableColumnHeader>
                    <Label>ClientVersion</Label>
                    <Alignment>Right</Alignment>
                    <Width>20</Width>
                </TableColumnHeader>
                <TableColumnHeader>
                    <Label>Obsolete</Label>
                    <Alignment>Right</Alignment>
                    <Width>15</Width>
                </TableColumnHeader>
                <TableColumnHeader>
                    <Label>CreationDate</Label>
                    <Alignment>Right</Alignment>
                    <Width>25</Width>
                </TableColumnHeader>
            </TableHeaders>
            <TableRowEntries>
                <TableRowEntry>
                    <TableColumnItems>
                        <TableColumnItem>
                            <PropertyName>Name</PropertyName>
                        </TableColumnItem>
                        <TableColumnItem>
                            <PropertyName>ResourceID</PropertyName>
                        </TableColumnItem>
                        <TableColumnItem>
                            <PropertyName>ClientVersion</PropertyName>
                        </TableColumnItem>
                        <TableColumnItem>

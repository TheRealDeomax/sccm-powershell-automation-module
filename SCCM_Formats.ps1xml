                        </ListItem>
                        <ListItem>
                            <PropertyName>LastLogonUserName</PropertyName>
                        </ListItem>
                    </ListItems>
                </ListEntry>  
            </ListEntries>
        </ListControl>
    </View>

    <View>
        <Name>System.Management.ManagementObject#SMS_R_System</Name>
        <ViewSelectedBy>
            <TypeName>System.Management.ManagementObject#SMS_R_System</TypeName>
        </ViewSelectedBy>
